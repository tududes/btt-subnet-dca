# Network settings
# Use 'finney' for mainnet or 'ws://127.0.0.1:9944' for local subtensor
SUBTENSOR=finney

# Block time in seconds (default: 12)
BLOCK_TIME_SECONDS=12

# Trading settings
# Minimum TAO balance to maintain (default: 1.0)
SAFETY_BALANCE=10.0

# Target slippage precision in TAO (default: 0.0001)
# Example: 0.0001 TAO = $0.05 in slippage for $500 TAO
SLIPPAGE_PRECISION=0.0001

# Wallet passwords (optional)
# Format: BT_PW__ROOT__BITTENSOR_WALLETS_<wallet-name>_COLDKEY=<password>
BT_PW__ROOT__BITTENSOR_WALLETS_COLDKEY-01_COLDKEY=your-password-here 